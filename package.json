    "@react-native-community/slider": "^4.4.2",
    "@react-native-google-signin/google-signin": "^6.0.0",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/drawer": "^6.6.2",
    "@react-navigation/material-top-tabs": "^6.6.2",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@react-navigation/stack": "^6.3.16",
    "@reduxjs/toolkit": "^1.9.3",
    "base64-arraybuffer": "^1.0.2",
    "moment": "^2.29.4",
    "react": "18.1.0",
    "react-native": "0.70.6",
    "react-native-actionsheet": "^2.4.2",
    "react-native-compressor": "^1.6.1",
    "react-native-device-info": "^10.8.0",
    "react-native-document-picker": "^6.0.4",
    "react-native-element-dropdown": "^2.9.0",
    "react-native-fbsdk-next": "^6.2.0",
    "react-native-fs": "^2.20.0",
    "react-native-gesture-handler": "2.12.0",
    "react-native-iap": "^12.10.7",
    "react-native-image-crop-picker": "^0.40.0",
    "react-native-image-picker": "^5.3.1",
    "react-native-image-progress": "^1.2.0",
    "react-native-image-viewing": "^0.2.2",
    "react-native-image-zoom-viewer": "^3.0.1",
    "react-native-inappbrowser-reborn": "^3.7.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-localization": "^2.3.1",
    "react-native-modal-datetime-picker": "^15.0.0",
    "react-native-orientation-locker": "^1.5.0",
    "react-native-pell-rich-editor": "^1.9.0",
    "react-native-progress": "^5.0.0",
    "react-native-reanimated": "^2.9.1",
    "react-native-receive-sharing-intent": "^2.0.0",
    "react-native-render-html": "^6.3.4",
