    "react-native-splash-screen": "^3.3.0",
    "react-native-super-grid": "^5.0.0",
    "react-native-svg": "^12.3.0",
    "react-native-toast-message": "^2.1.6",
    "react-native-vector-icons": "^10.0.0",
    "react-native-video": "^5.2.1",
    "react-native-video-controls": "^2.8.1",
    "react-native-webview": "^12.0.2",
    "react-redux": "^8.0.5",
    "rn-fetch-blob": "^0.12.0",
    "styled-components": "^5.3.9"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^26.0.23",
    "@types/react": "^18.0.21",
    "@types/react-native": "^0.70.6",
    "@types/react-native-video": "^5.0.14",
    "@types/react-test-renderer": "^18.0.0",
    "@types/styled-components": "^5.1.26",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.32.0",
    "jest": "^26.6.3",
    "jest-svg-transformer": "^1.0.0",
    "jest-transform-stub": "^2.0.0",
    "metro-react-native-babel-preset": "0.72.3",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.1.0",
    "typescript": "^4.8.3"
  },
  "jest": {
    "preset": "react-native",
    "testEnvironment": "node",
    "verbose": true,
    "globals": {
      "__DEV__": true
    },
    "haste": {
      "defaultPlatform": "ios",
      "platforms": [
        "android",
        "ios",
        "native"
      ]
    },
    "setupFilesAfterEnv": [
      "./jest/setup.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/fileTransformer.js",
      "\\.(css|less)$": "<rootDir>/fileTransformer.js",
      "^components(.*)$": "<rootDir>/src/components$1",
      "^hooks(.*)$": "<rootDir>/src/hooks$1",
      "^screens(.*)$": "<rootDir>/src/screens$1"
    },
    "collectCoverage": true,
    "transformIgnorePatterns": [
      "node_modules/(?!(react-native)|(@react-navigation)|(native-base-.*)|(react-native-.*)|(prop-types)|(react-native-google-signin)|(@invertase/react-native-apple-authentication)|(@react-native-firebase/messaging)|(@react-native-firebase/app)|(@react-native-firebase/auth)|(@react-native-firebase/firestore)|(@react-native-community/push-notification-ios)|(@dudigital/react-native-zoomable-view)|(react-native-keychain)|(@haskkor/react-native-pincode)|(@expo/react-native-action-sheet)/)"
    ],
    "coverageReporters": [
      "html"
    ],
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest",
      "^.+\\.(js|jsx)$": "babel-jest"
    }
  }
}
